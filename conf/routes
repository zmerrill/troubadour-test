# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

#JSON APIs
POST	/api/hi						controllers.apis.ProjectAPI.sayHello()
POST	/api/getOwnedProjects		controllers.apis.ProjectAPI.getOwnedProjects()
POST	/api/getInvolvedProjects	controllers.apis.ProjectAPI.getInvolvedProjects()
POST	/api/createProject			controllers.apis.ProjectAPI.createProject()
POST	/api/findProjectById		controllers.apis.ProjectAPI.findProjectById()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
